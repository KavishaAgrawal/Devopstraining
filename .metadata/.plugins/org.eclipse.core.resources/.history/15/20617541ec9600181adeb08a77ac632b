
public class Demo2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
     int[][]arrays;
     arrays = new int [3][3];
     
     arrays[0][1]=1;
     arrays[0][2]=2;
     arrays[0][3]=3;
     
     arrays[1][1]=11;
     arrays[1][2]=12;
     arrays[1][3]=13;
     
     arrays[2][1]=21;
     arrays[2][2]=22;
     arrays[2][3]=23;
     
     for (int i=0;i<arrays.length;i++) {
    	 for (int j=0;j<arrays[i].length;j++) {
    		 System.out.print(arrays[i][j]+"\t");
    	 }
    	 System.out.println();
     }
     
	}

}
