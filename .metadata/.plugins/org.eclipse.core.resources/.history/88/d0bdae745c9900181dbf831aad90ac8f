package com.myabstract;

abstract public class Circle extends Shape {
    private int radius;
    public Circle() {
		// TODO Auto-generated constructor stub
	radius = 10;
    }
    	
  
	public Circle (int radius) {
		super();
		this.radius = radius;
	}



	@Override
	public double area() {
	  return (3.14*radius*radius);
  }
}
