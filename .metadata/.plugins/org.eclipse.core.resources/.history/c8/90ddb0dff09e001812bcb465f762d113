

	class MyThread1 extends Thread {
        public static void main(String [] args) {
              MyThread1 t = new MyThread1();
              Thread x1 = new Thread(t); 
               x1.start();
         }

         public void run() {
                  for(int i=0;i<3;++i) {
                        System.out.print(i + "..");
                  }
          }
}


